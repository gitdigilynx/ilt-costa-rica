{% extends "WicrewCoreBundle:Browsing:browsing.header.html.twig" %}
{% set backgroundImageURL = absolute_url(asset('bundles/wicrewcore/images/bg-shared-shuttles.jpg')) %}

{% block title %}
    {{ ('product.shared.head'|trans)|raw }}
{% endblock %}

{% block subTitle %}
    {% if subTitle is defined %}
        {{ subTitle }}
    {% else %}
        {{ ('product.shared.head.desc'|trans)|raw }}
    {% endif %}
{% endblock %}