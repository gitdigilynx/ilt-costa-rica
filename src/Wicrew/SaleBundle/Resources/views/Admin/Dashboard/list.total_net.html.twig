{% set november1st = '2023-11-01' %}
{% if item.pickDate|date('Y-m-d') >= november1st %}
    {# @var item \App\Wicrew\SaleBundle\Entity\OrderItem #}
    {% if item.type is same as "private_jbj" %}
        {% set total_passengers         = item.adultCount + item.childCount %}
        {% if total_passengers > 5 %}
            {% set additional_passengers    = total_passengers - 5 %}
        {% else %}
            {% set additional_passengers    = 0 %}        
        {% endif %}
        {% set additional_p_cost        = additional_passengers * 25 %}
        {% if  item.pickDate|date("Y") == 2023 and item.pickDate|date("m") <= 6 %}
            ${{  ( ( ("8.00" / 100) * ( item.product.novemberFixedNetPrice  + additional_p_cost ) ) + ( item.product.novemberFixedNetPrice  + additional_p_cost  ) )|number_format(2, '.', ',') }}
        {% else %}
            ${{  ( ( ("13.00" / 100) * ( item.product.novemberFixedNetPrice  + additional_p_cost ) ) + ( item.product.novemberFixedNetPrice  + additional_p_cost  ) )|number_format(2, '.', ',') }}
        {% endif %}
    {% elseif item.type is same as "private_shuttle" and item.product.novemberFixedNetPrice %}
        {% if item.product.novemberFixedNetPrice != 0 %}
            {% if  item.pickDate|date("Y") == 2023 and item.pickDate|date("m") <= 6 %}
                ${{  ( ( ("8.00" / 100) * item.product.novemberFixedNetPrice ) + ( item.product.novemberFixedNetPrice ) )|number_format(2, '.', ',') }}
            {% else %}
                ${{  ( ( ("13.00" / 100) * item.product.novemberFixedNetPrice ) + ( item.product.novemberFixedNetPrice ) )|number_format(2, '.', ',') }}
            {% endif %}
        {% else %}
            {% if  item.pickDate|date("Y") == 2023 and item.pickDate|date("m") <= 6 %}
                ${{  ( ( ("8.00" / 100) * item.product.fixedNetPrice ) + ( item.product.fixedNetPrice ) )|number_format(2, '.', ',') }}
            {% else %}
                ${{  ( ( ("13.00" / 100) * item.product.fixedNetPrice ) + ( item.product.fixedNetPrice ) )|number_format(2, '.', ',') }}
            {% endif %}
        {% endif %}
    {% elseif item.type is same as "activity_regular" and item.activity.adultNetPrice and item.activity.childNetPrice %}
        ${{ (( ("13.00" / 100) * ( (item.activity.adultNetPrice * item.adultCount) + (item.activity.childNetPrice * item.childCount) ) ) + ( (item.activity.adultNetPrice * item.adultCount) + (item.activity.childNetPrice * item.childCount) ))|number_format(2, '.', ',') }} 
    {% else %}
        ${{ ( ( ( item.taxValue / 100 ) * item.subtotalNet) + item.subtotalNet )|number_format(2, '.', ',') }}
    {% endif %}

{% else %}
    {# @var item \App\Wicrew\SaleBundle\Entity\OrderItem #}
    {% if item.type is same as "private_jbj" %}
        {% set total_passengers         = item.adultCount + item.childCount %}
        {% if total_passengers > 5 %}
            {% set additional_passengers    = total_passengers - 5 %}
        {% else %}
            {% set additional_passengers    = 0 %}        
        {% endif %}
        {% set additional_p_cost        = additional_passengers * 25 %}
        {% if  item.pickDate|date("Y") == 2023 and item.pickDate|date("m") <= 6 %}
            ${{  ( ( ("8.00" / 100) * ( item.product.fixedNetPrice  + additional_p_cost ) ) + ( item.product.fixedNetPrice  + additional_p_cost  ) )|number_format(2, '.', ',') }}
        {% else %}
            ${{  ( ( ("13.00" / 100) * ( item.product.fixedNetPrice  + additional_p_cost ) ) + ( item.product.fixedNetPrice  + additional_p_cost  ) )|number_format(2, '.', ',') }}
        {% endif %}       
    {% elseif item.type is same as "private_shuttle" and item.product.fixedNetPrice %}
        {% if  item.pickDate|date("Y") == 2023 and item.pickDate|date("m") <= 6 %}
            ${{  ( ( ("8.00" / 100) * item.product.fixedNetPrice ) + ( item.product.fixedNetPrice ) )|number_format(2, '.', ',') }}
        {% else %}
            ${{  ( ( ("13.00" / 100) * item.product.fixedNetPrice ) + ( item.product.fixedNetPrice ) )|number_format(2, '.', ',') }}
        {% endif %}
    {% elseif item.type is same as "activity_regular" and item.activity.adultNetPrice and item.activity.childNetPrice %}
        ${{ (( ("13.00" / 100) * ( (item.activity.adultNetPrice * item.adultCount) + (item.activity.childNetPrice * item.childCount) ) ) + ( (item.activity.adultNetPrice * item.adultCount) + (item.activity.childNetPrice * item.childCount) ))|number_format(2, '.', ',') }} 
    {% else %}
        ${{ ( ( ( item.taxValue / 100 ) * item.subtotalNet) + item.subtotalNet )|number_format(2, '.', ',') }}
    {% endif %}
{% endif %}