{% block javascripts %}
    
    {{ parent() }}

    {% if areaFrom_id is defined %}
        <script>
            $(document).ready(function () {
                var options = $('select#departure-at option');
                var values = $.map(options ,function(option) {
                    return option;
                });
                $( values ).each(function( index, element ) {
                    if(element == ''){
                        return true;
                    }
                    element = $.parseJSON(element);
                    $( element ).each(function( key, select_val ) {
                        if( ( select_val.fromID == {{ areaFrom_id }} ) && ( select_val.toID == {{ areaTo_id }} ) ){
                            $("select#departure-at").val( JSON.stringify(select_val) ).trigger('change');
                        }
                    });
                });
            });
        </script>
    {% endif %}
    {% if dl_childAreas is defined and dl_childAreas|length > 0 %} {# SELECTING THE CHILD AREA IS EXISTS #}
    <script>
        $(document).ready(function () {
            let dl_childAreas = {{dl_childAreas|json_encode|raw}};
            $.each(dl_childAreas, function( trip_id, children_areas ) {
                if ('child_area_from' in children_areas) {
                    var childsArray = $.map(children_areas.child_area_from, function(value, key) {
                        return value;
                    });
                    
                    var firstChild = childsArray[0];
                    $('input#additionalFee_search_from_'+trip_id).val(firstChild).change();
                }
                if ('child_area_to' in children_areas) {
                    var childstoArray = $.map(children_areas.child_area_to, function(value, key) {
                        return value;
                    });
                    
                    var firsttoChild = childstoArray[0];
                    $('input#additionalFee_search_to_'+trip_id).val(firsttoChild).change();
                }
            });
        });
    </script>
    {% endif %} {# SELECTING THE CHILD AREA IS EXISTS #}
    

